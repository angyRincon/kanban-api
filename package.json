{
  "name": "server",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "test": "jest",
    "dev": "dotenvx run --env-file=.env.local -- ts-node-dev ./src/server.ts",
    "start": "dotenvx run --env-file=.env.production -- ts-node-dev ./src/server.ts",
    "typeorm": "typeorm-ts-node-commonjs -d ./src/typeorm.ts",
    "migration:create": "cd ./src/migrations && typeorm-ts-node-commonjs migration:create",
    "migration:run": "npm run typeorm migration:run",
    "migration:revert": "npm run typeorm migration:revert"
  },
  "dependencies": {
    "express": "^4.18.2",
    "express-validator": "^7.0.1",
    "pg": "^8.11.3",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.0",
    "typeorm": "^0.3.19"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.23.9",
    "@dotenvx/dotenvx": "^0.14.1",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.11",
    "@types/node": "^20.11.6",
    "@types/supertest": "^6.0.2",
    "@types/swagger-jsdoc": "^6.0.4",
    "@types/swagger-ui-express": "^4.1.6",
    "babel-jest": "^29.7.0",
    "dotenv": "^16.4.1",
    "jest": "^29.7.0",
    "supertest": "^6.3.4",
    "ts-jest": "^29.1.2",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.3.3"
  }
}
